                  <xs:annotation>
                     <xs:documentation>specifies the type of measurement in any convenient typology.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:token">
                        <xs:pattern value="(\p{L}|\p{N}|\p{P}|\p{S})+"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
