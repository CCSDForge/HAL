         </xs:annotation>
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:pattern value="(\p{L}|\p{N}|\p{P}|\p{S})+"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
